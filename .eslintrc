{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "browser": true,
        "amd": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "quotes": [ 1, "double" ],
        "no-undef": 1,
        "no-extra-semi": 1,
        "eqeqeq": 1,
        "dot-notation": 1,
        "consistent-return": 1,
        "no-alert": 1,
        "no-else-return": 1,
        "no-empty-pattern": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-invalid-this": 1,
        "no-loop-func": 1,
        "no-magic-numbers": 1,
        "no-new-wrappers": 1,
        "no-redeclare": 1,
        "no-return-assign": 1,
        "strict": [2, "global"],
        "yoda": 0,

        "array-bracket-spacing": [1, "always"],
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 1,
        "comma-spacing": [1, { "before": false, "after": true }],
        "comma-style": [1, "last"],
        "func-names": 1,
        "key-spacing": [1, { "beforeColon": false, "afterColon": true }],
        "new-parens": 1,
        "no-continue": 1,
        "object-curly-spacing": [1, "always"],
        "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
        "no-multiple-empty-lines": [1, { "max": 1 }],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-trailing-spaces": 1,
        "one-var": [1, "always"],
        "space-before-blocks": 1,
        "jsx-quotes": 1

    }
}
